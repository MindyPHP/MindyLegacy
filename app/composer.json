{
  "name": "mindy/mindy",
  "type": "metapackage",
  "homepage": "http://mindy-cms.com",
  "version": "1",
  "license": "BSD-3-Clause",
  "autoload": {
    "psr-4": {
      "Modules\\": "Modules"
    }
  },
  "config": {
    "cache-files-ttl": 0
  },
  "require": {
    "php": ">=7",
    "mindy/orm": "*",
