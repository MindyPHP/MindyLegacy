    "mindy/event": "*",
    "mindy/application": "*",
    "mindy/table": "*"
  },
  "require-dev": {
    "mindy/tests": "*",
    "phpunit/phpunit": "3.7.32",
    "phpunit/php-invoker": "1.1.3",
    "phpunit/dbunit": "1.3.1",
    "phpunit/phpunit-selenium": "1.3.3",
    "phpunit/phpunit-story": "1.0.2",
    "satooshi/php-coveralls": "dev-master"
